{
  "name": "glowgaia/grabbit",
  "description": "An extremely specific wrapper around Laravel's Illuminate HTTP Client that allows for short and expressive requests to Gaia Online's GSI API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sheldon Corcoran",
      "email": "awesomolocity@gmail.com"
    }
  ],
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "GlowGaia\\Grabbit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GlowGaia\\Grabbit\\Tests\\": "tests/"
    }
  },
  "require": {
    "saloonphp/saloon": "^3.0",
    "php": "^8.4",
    "illuminate/collections": "^12.28"
  },
  "require-dev": {
    "phpunit/phpunit": "^12",
    "tightenco/duster": "^3.2"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "duster:fix": "./vendor/bin/duster fix",
    "duster:lint" : "./vendor/bin/duster lint"
  }
}
